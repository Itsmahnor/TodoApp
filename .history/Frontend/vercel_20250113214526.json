{
  "version": 2,
  "builds": [
    {
      "src": "server/Frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Frontend/dist"
      }
    },
    {
      "src": "server/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server/Frontend/dist/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/app.js"
    }
  ]
}
