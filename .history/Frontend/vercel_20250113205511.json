{
  "version": 2,
  "builds": [
    {
      "src": "backend/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "backend/frontend/dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/backend/frontend/dist/$1"
    }
  ]
}
